print('perkenalan')
nama = input('Nama saya: ')
umur = int(input('Umur saya: '))
print('Hallo nama saya {} umur saya {}'.format(nama, umur))

print(nama, type(nama))
print(umur, type(umur))

#ubah tipe data float = float(a) long = long(a) string = str(a)
print('Hallo nama saya {} umur saya {}'.format(nama, int(umur)))
print(umur, type(umur))

